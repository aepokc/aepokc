<!DOCTYPE html>
<html>
	<head>
		<title>aep | the Alliance of Emerging Professionals</title>
		<%= stylesheet_link_tag "application", "common", "style" %>
		<%= stylesheet_link_tag "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.2/themes/base/jquery-ui.css" %>
		<%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js" %>
		<%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.2/jquery-ui.min.js" %>
		<%= javascript_include_tag "jquery.rails.js" %>
		<%= javascript_include_tag "application.js" %>
		<%= javascript_include_tag "analytics.js" %>
		<%= csrf_meta_tag %>
	</head>
	<body>
		<div class="signup" >
			<%= link_to "", 'http://visitor.constantcontact.com/email.jsp?m=1101983411682&lang=en' %>
		</div>
		<div class="wrapper">
			<div class="beta" >
				<%= image_tag("beta.png") %>
			</div>
			<%= render "/partials/logout" %>
			<div class="header">
			</div>
			<%= render "/partials/navbar" %>
			<div class="content">
				<div class="contentlt">
					<div class="ltheader font11">
						Admin
					</div>
					<div class="ltnav font11">
						<div class="ltnavitem">
							<% if controller_name == "pages" %>
								<% if action_name == "admin" %>
									<%= link_to "Pages", "/pages" %>
								<% else %>
									Pages
								<% end %>
							<% else %>
								<%= link_to "Pages", "/pages" %>
							<% end %>
						</div>
						<div class="ltnavitem">
							<% if controller_name == "registrations" %>
								Members
							<% else %>
								<%= link_to "Members", "/members" %>
							<% end %>
						</div>
						<div class="ltnavitem">
							<% if controller_name == "membership_applications" %>
								Applicants
							<% else %>
								<%= link_to "Applicants", "/membership/applications" %>
							<% end %>
						</div>
					</div>
					<div class="clear">
					</div>
				</div>
				<div class="contentrt font11">
					<div class="flash">
						<%= flash[:notice] %>
						<%= flash[:alert] %>
					</div>
					<%= yield %>
				</div>
				<div class="clear">
				</div>
			</div>
		</div>
		<%= render "/partials/footer" %>
	</body>
</html>
